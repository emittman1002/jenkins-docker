clear

c=`docker ps | grep emittman1002/jenkins`
if [ -n "$c" ]
then
    c=`echo $c | awk '{print $1}'`
    echo Stopping container \"$c\"
    docker stop $c
fi

c=`docker ps -a | grep emittman1002/jenkins`
if [ -n "$c" ]
then
    c=`echo $c | awk '{print $1}'`
    echo Deleting container \"$c\"
    docker rm $c
fi

i=`docker images | grep emittman1002/jenkins`
if [ -n "$i" ]
then
    i=`echo $i | awk '{print $3}'`
    echo Deleting image \"$i\"
    docker image rm $i
fi
